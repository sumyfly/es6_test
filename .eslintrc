{
  "env": {
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "no-case-declarations": "warn",
    "no-unused-vars": "warn",
    "no-console": 0,
    "no-debugger": 1,
    "comma-dangle": 0
  },
  "extends": "eslint:recommended",
  "globals": {
    "Promise": true
  }
}